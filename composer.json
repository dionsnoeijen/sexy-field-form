{
  "name": "tardigrades/sexy-field-form",
  "description": "SexyField Form. Generate symfony forms based on section configuration.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/orm": "^2.5.6",
    "symfony/form": "^3.3.2",
    "symfony/validator": "^3.3.2",
    "symfony/security": "^3.3.2",
    "symfony/dependency-injection": "^3.3.2",
    "symfony/yaml": "^3.3.2",
    "symfony/config": "^3.3.2",
    "twig/twig": "^2.0",
    "twig/extensions": "^1.5.1",
    "php-coveralls/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "require-dev": {
    "tardigrades/sexy-field-field-types-base": "dev-master",
    "tardigrades/sexy-field": "dev-master",
    "phpunit/phpunit": "^6.1",
    "squizlabs/PHP_CodeSniffer": "^2.8",
    "mockery/mockery": "^0.9.9",
    "mikey179/vfsStream": "^1.6"
  }
}
