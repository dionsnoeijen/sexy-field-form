{
  "name": "tardigrades/sexy-field-form",
  "description": "SexyField Form. Generate symfony forms based on section configuration.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "^7.4",
    "doctrine/orm": "^2.8",
    "symfony/form": "^5",
    "symfony/validator": "^5",
    "symfony/dependency-injection": "^5",
    "symfony/yaml": "^5",
    "symfony/config": "^5",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "require-dev": {
    "tardigrades/sexy-field-field-types-base": "dev-sym5",
    "tardigrades/sexy-field": "dev-sym5",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0",
    "mikey179/vfsstream": "^1.6",
    "squizlabs/php_codesniffer": "^3.2"
  }
}
