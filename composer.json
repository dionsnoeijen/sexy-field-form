{
  "name": "tardigrades/sexy-field-form",
  "description": "SexyField Form. Generate symfony forms based on section configuration.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/orm": "^2.5.6",
    "symfony/form": "^4",
    "symfony/validator": "^4",
    "symfony/security": "^4",
    "symfony/dependency-injection": "^4",
    "symfony/yaml": "^4",
    "symfony/config": "^4",
    "twig/twig": "^2.0",
    "twig/extensions": "^1.5.1",
    "php-coveralls/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "require-dev": {
    "tardigrades/sexy-field-field-types-base": "dev-integrate-create-and-delete-options",
    "tardigrades/sexy-field": "dev-integrate-create-and-delete-options",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0",
    "mikey179/vfsstream": "^1.6",
    "squizlabs/php_codesniffer": "^3.2"
  }
}
